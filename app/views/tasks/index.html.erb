<p style="color: green"><%= notice %></p>

<h1>Tasks</h1>

<table id="tasks">
  <thead>
    <th>Title</th>
    <th>Content</th>
  </thead>
  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td>
          <%= link_to task.title, task %>
        </td>
        <td>
          <%= task.content %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to "New", new_task_path %>
