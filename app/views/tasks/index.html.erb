<p style="color: green"><%= notice %></p>

<h1><%= t(:tasklist) %></h1>

<table id="tasks">
  <thead>
    <th><%= t(:title)%></th>
    <th><%= t(:content)%></th>
  </thead>
  <tbody>
    <% @tasks.each do |task| %>
      <tr>
        <td>
          <%= link_to task.title, task %>
        </td>
        <td>
          <%= task.content %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to t(:new), new_task_path %>
